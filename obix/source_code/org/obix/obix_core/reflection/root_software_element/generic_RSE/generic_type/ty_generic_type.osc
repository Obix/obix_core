type generic_type

	inherit generic_RSE
		attribute id_path type:generic_type_id_path end
		attribute a_id type:generic_type_id end

		command is_equal_to
			in other type:generic_type end
		end
	end
	
	attribute base_type_id_path type:type_id_path end

end type